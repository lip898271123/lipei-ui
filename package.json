{
  "name": "lipei-ui",
  "version": "1.0.16",
  "description": "github开源组件库",
  "main": "lib/lipei-ui.common.js",
  "files": [
    "lib",
    "src",
    "packages"
  ],
  "scripts": {
    "bootstrap": "yarn || npm i",
    "dev": "npm run build:entry && webpack-dev-server --config build/webpack.dev.js",
    "build:utils": "cross-env BABEL_ENV=utils babel src --out-dir lib --ignore src/index.js",
    "build:style": "node build/gen-cssfile && gulp build --gulpfile packages/style/gulpfile.js && cp-cli packages/style/lib lib/style",
    "clean": "rimraf lib && rimraf packages/*/lib && rimraf test/**/coverage",
    "build": "npm run clean && npm run build:entry && webpack --config build/webpack.conf.js && webpack --config build/webpack.common.js && webpack --config build/webpack.component.js && npm run build:style && npm run build:utils",
    "lint": "eslint src/**/* test/**/* packages/**/* build/**/* --quiet",
    "build:entry": "node build/build-entry.js"
  },
  "repository": {
    "type": "git",
    "url": "git"
  },
  "keywords": [
    "lipei",
    "vue",
    "components"
  ],
  "author": "lipei ace",
  "license": "MIT",
